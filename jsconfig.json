{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#api/*": [
        "./src/api/*.js"
      ],
      "#components/*": [
        "./src/components/*/index.js"
      ],
      "#constants/*": [
        "./src/constants/*.js"
      ],
      "#helpers/*": [
        "./src/helpers/*/index.js"
      ],
      "#hooks/*": [
        "./src/hooks/*.js"
      ]
    }
  }
}
